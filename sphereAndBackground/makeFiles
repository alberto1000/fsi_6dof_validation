cat log.topoSet | grep -w 'Time =' | cut -d' ' -f3 | tr -d '(' > time
cat log.topoSet | grep 'of mass' | cut -d' ' -f8 | tr -d '('  > xcenter
cat log.topoSet | grep 'of mass' | cut -d' ' -f9 | tr -d '('  > ycenter
cat log.topoSet | grep 'of mass' | cut -d' ' -f10 | tr -d ')'  > zcenter
cat log.topoSet | grep 'Linear' | cut -d' ' -f7 | tr -d '(' > vx
cat log.topoSet | grep 'Linear' | cut -d' ' -f8 | tr -d ' ' > vy
cat log.topoSet | grep 'Linear' | cut -d' ' -f9 | tr -d ')' > vz

paste time xcenter ycenter zcenter vx vy vz > plotfile
