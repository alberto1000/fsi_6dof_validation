FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wall
    {
        type            slip;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    oversetPatch
    {
        type            overset;
        value           nonuniform List<vector> 0();
    }
    seed
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    procBoundary21to12
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary21to13
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary21to14
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary21to15
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary21to19
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary21to27
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary21to29
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


