FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 1 -1 0 0 0 0];

internalField    uniform $flowVelocity;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    overset
    {
        type            overset;
    }

    seed
    {
        type            movingWallVelocity;
        value           uniform (0 0 0); 
    }
    
    wall
    {
        type            fixedValue;  
        value           uniform (0 0 0); 
    }
    
     ".*"
    {
        type            zeroGradient;
    }

}
