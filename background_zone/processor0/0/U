FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.8);

boundaryField
{
    wall
    {
        type            noSlip;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0.8);
    }
    outlet
    {
        type            zeroGradient;
    }
    oversetPatch
    {
        type            overset;
        value           uniform (0 0 0.8);
    }
    seed
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0.8);
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform (0 0 0.8);
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform (0 0 0.8);
    }
    procBoundary0to6
    {
        type            processor;
        value           uniform (0 0 0.8);
    }
    procBoundary0to7
    {
        type            processor;
        value           uniform (0 0 0.8);
    }
    procBoundary0to9
    {
        type            processor;
        value           uniform (0 0 0.8);
    }
}


