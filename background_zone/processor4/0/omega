FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1.78;

boundaryField
{
    wall
    {
        type            omegaWallFunction;
        value           nonuniform List<scalar> 0();
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    oversetPatch
    {
        type            overset;
        value           nonuniform List<scalar> 0();
    }
    seed
    {
        type            omegaWallFunction;
        value           uniform 1.78;
    }
    procBoundary4to1
    {
        type            processor;
        value           uniform 1.78;
    }
    procBoundary4to2
    {
        type            processor;
        value           uniform 1.78;
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 1.78;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 1.78;
    }
    procBoundary4to7
    {
        type            processor;
        value           uniform 1.78;
    }
    procBoundary4to8
    {
        type            processor;
        value           uniform 1.78;
    }
    procBoundary4to10
    {
        type            processor;
        value           uniform 1.78;
    }
}


