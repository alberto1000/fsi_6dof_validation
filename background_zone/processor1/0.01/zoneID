FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.01";
    object      zoneID;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    wall
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    oversetPatch
    {
        type            overset;
        value           nonuniform List<scalar> 0();
    }
    seed
    {
        type            zeroGradient;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary1to4
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary1to7
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary1to10
    {
        type            processor;
        value           uniform 1;
    }
}


