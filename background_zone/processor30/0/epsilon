FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.88;

boundaryField
{
    wall
    {
        type            epsilonWallFunction;
        value           uniform 0.88;
    }
    inlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.01;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.88;
        value           uniform 0.88;
    }
    oversetPatch
    {
        type            overset;
        value           nonuniform List<scalar> 0();
    }
    seed
    {
        type            zeroGradient;
    }
    procBoundary30to22
    {
        type            processor;
        value           uniform 0.88;
    }
    procBoundary30to23
    {
        type            processor;
        value           uniform 0.88;
    }
    procBoundary30to24
    {
        type            processor;
        value           uniform 0.88;
    }
    procBoundary30to26
    {
        type            processor;
        value           uniform 0.88;
    }
    procBoundary30to31
    {
        type            processor;
        value           uniform 0.88;
    }
}


