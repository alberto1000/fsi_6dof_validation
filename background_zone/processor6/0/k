FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.5;

boundaryField
{
    wall
    {
        type            kqRWallFunction;
        value           uniform 1.5;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1.5;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    oversetPatch
    {
        type            overset;
        value           uniform 1.5;
    }
    seed
    {
        type            zeroGradient;
    }
    procBoundary6to0
    {
        type            processor;
        value           uniform 1.5;
    }
    procBoundary6to4
    {
        type            processor;
        value           uniform 1.5;
    }
    procBoundary6to5
    {
        type            processor;
        value           uniform 1.5;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 1.5;
    }
    procBoundary6to14
    {
        type            processor;
        value           uniform 1.5;
    }
    procBoundary6to15
    {
        type            processor;
        value           uniform 1.5;
    }
}


